[ignore]
<PROJECT_ROOT>/public.*

[include]

[libs]
flow-typed

[options]
suppress_comment= \\(.\\|\n\\)*\\flow-disable .+
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=warn
module.name_mapper='^.*\.scss$' -> '<PROJECT_ROOT>/src/ignoreExtensions'
module.name_mapper='^.*\.css$' -> '<PROJECT_ROOT>/src/ignoreExtensions'
module.system.node.resolve_dirname=node_modules